{
  "manifest_version": 2,
  "name": "Button Click Logger",
  "version": "1.0",
  "description": "Logs 'test' to console on button click",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}
